<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.callRing.common.mapper.IntersetDao">
  <resultMap id="BaseResultMap" type="com.callRing.common.po.Interset">
    <result column="id" jdbcType="VARCHAR" property="id" />
    <result column="name" jdbcType="VARCHAR" property="name" />
    <result column="hot_Word" jdbcType="VARCHAR" property="hotWord" />
    <result column="fir_Time" jdbcType="DATE" property="firTime" />
    <result column="last_up_time" jdbcType="DATE" property="lastUpTime" />
  </resultMap>
  <insert id="save" parameterType="com.callRing.common.po.Interset">
    insert into interset (id, name, hot_Word, 
      fir_Time, last_up_time)
    values (#{id,jdbcType=VARCHAR}, #{name,jdbcType=VARCHAR}, #{hotWord,jdbcType=VARCHAR}, 
      #{firTime,jdbcType=DATE}, #{lastUpTime,jdbcType=DATE})
  </insert>
  <insert id="saveSelective" parameterType="com.callRing.common.po.Interset">
    insert into interset
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="id != null">
        id,
      </if>
      <if test="name != null">
        name,
      </if>
      <if test="hotWord != null">
        hot_Word,
      </if>
      <if test="firTime != null">
        fir_Time,
      </if>
      <if test="lastUpTime != null">
        last_up_time,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="id != null">
        #{id,jdbcType=VARCHAR},
      </if>
      <if test="name != null">
        #{name,jdbcType=VARCHAR},
      </if>
      <if test="hotWord != null">
        #{hotWord,jdbcType=VARCHAR},
      </if>
      <if test="firTime != null">
        #{firTime,jdbcType=DATE},
      </if>
      <if test="lastUpTime != null">
        #{lastUpTime,jdbcType=DATE},
      </if>
    </trim>
  </insert>
  <insert id="batchSave" parameterType="com.callRing.common.po.Interset">
    insert into interset (id, name, hot_Word, fir_Time,last_up_time)
    values
    <foreach collection="list" index="index" item="interset" separator=",">
      (#{interset.id,jdbcType=VARCHAR},
      #{interset.name,jdbcType=VARCHAR},
      #{interset.hotWord,jdbcType=VARCHAR},
      #{interset.firTime,jdbcType=DATE},
      #{interset.lastUpTime,jdbcType=DATE})
    </foreach>
  </insert>
</mapper>